n the bootstrap.\n    sed -i \"$i\" -e \"s^/bin/pwd^$PWD_P^g\"\ndone\n\nmkdir ../build\ncd ../build\n\nconfigureScript=\"`pwd`/../$sourceRoot/configure\"\n\nmakeFlags=\"$makeFlags BUILD_LDFLAGS=-Wl,-rpath,/1sa4lg73vpayn1rc63xl7pz3c24xj18halziqmqyr3qqw7rap19z/lib OBJDUMP=/1r0q7096l3rx2vraw93brq49784mkx8m2lk12cc1xzq5n79q28g9/bin/objdump\"\n\n\n"),("propagatedBuildInputs",""),("propagatedNativeBuildInputs",""),("setupHook","/0s0w5yrrkyrb4sqd4k2rwnvv9jrjns89pzf0p50lxfx7kvlydnsi"),("src","/nix/store/l66b2j7crhj29wk7sx4nycbq55np9fvh-glibc-2.40.tar.xz"),("stdenv","/1aipp690wc31xkcjpxmf60p9jxvr9pz0nhlh5z0f1lh095syp33k"),("strictDeps","1"),("system","x86_64-linux"),("version","2.40-36")])